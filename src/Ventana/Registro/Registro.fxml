<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" fx:id="pane" prefHeight="517.0" prefWidth="342.0"
            style="-fx-background-color: #2D2341;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="Ventana.Registro.Registro">
    <stylesheets>
        <URL value="@Registro.css"/>
    </stylesheets>
    <children>
        <StackPane fx:id="content" prefHeight="518.0" prefWidth="342.0">
            <children>
                <Pane prefHeight="513.0" prefWidth="342.0">
                    <children>
                        <JFXButton alignment="CENTER_RIGHT" focusTraversable="false" layoutX="9.0" layoutY="9.0"
                                   onAction="#volverClick" prefHeight="31.0" prefWidth="139.0" ripplerFill="WHITE"
                                   text="Volver al inicio" textFill="WHITE">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXButton>
                        <ImageView fitHeight="20.0" fitWidth="25.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@img/exiticon.png"/>
                            </image>
                        </ImageView>
                        <ImageView fitHeight="95.0" fitWidth="167.0" layoutX="124.0" layoutY="44.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@img/icon.png"/>
                            </image>
                        </ImageView>
                        <Label focusTraversable="false" layoutX="85.0" layoutY="135.0" text="Human" textFill="WHITE">
                            <font>
                                <Font name="Segoe UI Light" size="30.0"/>
                            </font>
                        </Label>
                        <Label focusTraversable="false" layoutX="177.0" layoutY="139.0" text="-Ware" textFill="#80e5ff">
                            <font>
                                <Font name="Segoe UI" size="30.0"/>
                            </font>
                        </Label>
                        <JFXTextField fx:id="usuario" focusColor="#80e5ff" layoutX="60.0" layoutY="217.0"
                                      prefHeight="31.0" prefWidth="268.0" promptText="Usuario"
                                      style="-fx-text-fill: #ffffff;" unFocusColor="WHITE">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXTextField>
                        <JFXPasswordField fx:id="contrasena" focusColor="#80e5ff" layoutX="60.0" layoutY="298.0"
                                          prefHeight="31.0" prefWidth="268.0" promptText="Contraseña"
                                          style="-fx-text-fill: #ffffff;" unFocusColor="WHITE">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXPasswordField>
                        <JFXPasswordField fx:id="contrasena1" focusColor="#80e5ff" layoutX="60.0" layoutY="341.0"
                                          prefHeight="31.0" prefWidth="268.0"
                                          promptText="Ingrese nuevamente la contraseña" style="-fx-text-fill: #ffffff;"
                                          unFocusColor="WHITE">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXPasswordField>
                        <JFXComboBox fx:id="tipo" focusColor="#80e5ff" layoutX="60.0" layoutY="416.0"
                                     onAction="#cambioTipo" prefHeight="31.0" prefWidth="268.0"
                                     promptText="Tipo de cuenta"
                                     style="-fx-font-size: 15; -fx-text-fill: #ffffff; -fx-font-family: &quot;Segoe UI&quot;;"
                                     unFocusColor="WHITE"/>
                        <JFXButton layoutX="235.0" layoutY="474.0" onAction="#registrarClick" ripplerFill="#80e5ff"
                                   style="-fx-background-color: #80e5ff;" text="Registrarse">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXButton>
                        <ImageView fitHeight="31.0" fitWidth="33.0" layoutX="14.0" layoutY="217.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@img/user.png"/>
                            </image>
                        </ImageView>
                        <ImageView fitHeight="31.0" fitWidth="33.0" layoutX="14.0" layoutY="298.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@img/pass.png"/>
                            </image>
                        </ImageView>
                        <JFXButton alignment="CENTER_RIGHT" focusTraversable="false" layoutX="186.0" layoutY="9.0"
                                   onAction="#cerrarClick" prefHeight="31.0" prefWidth="147.0" ripplerFill="WHITE"
                                   text="Cerrar la aplicación" textFill="WHITE">
                            <font>
                                <Font name="Segoe UI" size="15.0"/>
                            </font>
                        </JFXButton>
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
